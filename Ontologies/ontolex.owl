    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/lemon/ontolex#otherForm -->

    <owl:ObjectProperty rdf:about="&ontolex;otherForm">
        <rdfs:comment rdf:datatype="&xsd;string">A non-preferred (&quot;non-dictionary&quot;) representation of a lexical entry. This should be a variant that is either a morphological variant, an abbreviation, short form or acronym</rdfs:comment>
        <rdfs:range rdf:resource="&ontolex;Form"/>
        <rdfs:domain rdf:resource="&ontolex;LexicalEntry"/>
