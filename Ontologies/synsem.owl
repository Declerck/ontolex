        <rdfs:subClassOf rdf:resource="&synsem;Argument"/>
        <rdfs:comment xml:lang="en">The class SemanticArgument represents a slot that needs to be filled for a semantic frame to be complete. Semantic arguments typically express some actor or role played by some entity in the given semantic frame.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/lemon/synsem.owl#SemanticFrame -->

    <owl:Class rdf:about="&synsem;SemanticFrame">
        <rdfs:label xml:lang="en">Semantic Frame</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
