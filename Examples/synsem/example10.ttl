#This example models two lexical entries, buy and sell, as referring to the same reference <http://myexample/exchange_goods_for_money_event>. The example uses the class SemanticFrame to specify that both lexical entries refer to the same frame, albeit with different argument bindings.

@prefix ontolex: <http://www.w3.org/ns/lemon/ontolex#> .
@prefix synsem: <http://www.w3.org/ns/lemon/synsem#> .
@prefix lexinfo: <http://lexinfo.net/ontology/2.0/lexinfo.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

@prefix : <> .



:sell a ontolex:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  ontolex:canonicalForm :buy_canonical_form;
  synsem:synBehavior :sell_transitive_pp;
  ontolex:sense :selling.

:selling ontolex:reference <http://myexample/exchange_goods_for_money_event>.

:sell_canonical_form ontolex:writtenRep "sell"@en.

:sell_transitive_pp a lexinfo:TransitivePPFrame;
 lexinfo:subject  	      :sell_arg1 ;
 lexinfo:directObject         :sell_arg2 ;
 lexinfo:prepositionalAdjunct :sell_arg3;
 synsem:frame :exchange_goods_for_money_frame.

:sell_arg3 synsem:marker :to ;
             synsem:optional "true"^^xsd:boolean .


:exchange_goods_for_money_semframe a synsem:SemanticFrame;
synsem:subFrame :sell_transitive_pp_subframe1;
synsem:subFrame :sell_transitive_pp_subframe2;
synsem:subFrame :sell_transitive_pp_subframe3;
ontolex:sense	:selling.

:sell_transitive_subframe1 ontolex:reference <http://myexample/seller>;
                                 synsem:frameArg  :sell_arg1.

:sell_transitive_subframe2 ontolex:reference <http://myexample/exchangedGood>;
                                 synsem:frameArg :sell_arg2. 

:sell_transitive_subframe3 ontolex:reference <http://myexample/buyer>;
                                 synsem:frameArg  :sell_arg3.

:buy a ontolex:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  ontolex:canonicalForm :buy_canonical_form;
  synsem:synBehavior :buy_transitive_pp;
  ontolex:sense :buying.

:buying ontolex:reference <http://myexample/exchange_goods_for_money_event>.

:buy_canonical_form ontolex:writtenRep "buy"@en.

:buy_transitive_pp a lexinfo:TransitivePPFrame;
 lexinfo:subject              :buy_arg1 ;
 lexinfo:directObject         :buy_arg2 ;
 lexinfo:prepositionalAdjunct :buy_arg3;
 synsem:frame :exchange_goods_for_money_frame.

:buy_arg3 synsem:marker :from ;
             synsem:optional "true"^^xsd:boolean .


:exchange_goods_for_money_semframe a synsem:SemanticFrame;
synsem:subFrame :buy_transitive_pp_subframe1;
synsem:subFrame :buy_transitive_pp_subframe2;
synsem:subFrame :buy_transitive_pp_subframe3;
ontolex:sense   :buying.

:buy_transitive_pp_subframe1 ontolex:reference <http://myexample/seller>;
                                 synsem:frameArg  :buy_arg3.

:buy_transitive_pp_subframe2 ontolex:reference <http://myexample/exchangedGood>;
                                 synsem:frameArg :buy_arg2.

:buy_transitive_pp_subframe3 ontolex:reference <http://myexample/buyer>;
                                 synsem:frameArg  :buy_arg1.


